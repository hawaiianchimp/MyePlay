<!DOCTYPE html>
<html>
<head>
  <title>Video.js | HTML5 Video Player</title>

  <!-- Chang URLs to wherever Video.js files will be hosted -->
  <link href="/css/video-js.css" rel="stylesheet" type="text/css">
  <link href="/css/myeplay.css" rel="stylesheet" type="text/css">
  <link href="/css/bootstrap.css" rel="stylesheet" type="text/css">
  <!-- video.js must be in the <head> for older IEs to work. -->

  <script src="/js/jquery.js"></script>
  <script src="/js/bootstrap.js"></script>
  <script src="http://localhost:6556/eyetribe.js"></script>
  <script src="/js/video.js"></script>
  <script src="/js/vjs.youtube.js"></script>
 
  <script src="/js/mediaPlayer.js"></script>
  
  <!-- Unless using the CDN hosted version, update the URL to the Flash SWF -->
</head>
<body>
	
	<video id="video" 
	  class="video-js vjs-default-skin" 
	  autoplay="true"
	  preload="none" 
	  width="100%" 
	  height="600"
		poster=""
	  data-setup='{"techOrder":["youtube"], "src":"http://www.youtube.com/watch?v={{videoId}}"}'>
  </video>
 
 <script src="/js/controls.js"></script>
</body>
</html>
